{
    "name": "europeana/members-addons",
    "description": "Membership add-on customisations for HIM",
    "type": "bolt-extension",
    "keywords": [
        "addon"
    ],
    "require": {
        "php": ">=5.5.9",
        "bolt/bolt": "^3.0",
        "bolt/members": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Europeana\\MembersAddons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Europeana\\MembersAddons\\Tests\\": "tests"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Europeana\\MembersAddons\\MembersAddonsExtension",
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
